  },
  "devDependencies": {
    "@strapi/strapi": "^5.10.3",
    "@strapi/sdk-plugin": "^5.3.2",
    "prettier": "^3.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.29.0",
    "styled-components": "^6.1.15",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
